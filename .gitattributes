# Set the default behavior, in case people don't have core.autocrlf set.
* text=auto

# These files are using CRLF
*.cfg text eol=crlf
*.dat text eol=crlf
*.frag text eol=crlf
*.java text eol=crlf
*.md text eol=crlf
*.properties text eol=crlf
*.svg text eol=crlf
*.txt text eol=crlf
*.vert text eol=crlf
*.yml text eol=crlf
/.project text eol=crlf
/LICENSE text eol=crlf

# These files are using LF
*.sh text eol=lf
*.xml text eol=lf
/.gitattributes text eol=lf
/.gitignore text eol=lf

# These are binary files
*.class binary
*.icns binary
*.ico binary
*.jar binary
*.png binary
*.zip binary
